{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/*.js.map"
    ],
    "rewrites": [
      {
        "source": "/p/**",
        "function": "ssrapp"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
